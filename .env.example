# Copy this to .env and set values (never commit real credentials)
NEO4J_URI=neo4j+s://YOUR_AURA_OR_BOLT_URI_HERE
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-password
PORT=8080
# Optional: permit write endpoints (PATCH/POST). Default false to keep things safe by default.
ENABLE_WRITES=false

WEBSITE_NODE_DEFAULT_VERSION=~20

AUTH_REQUIRED=<true|false>
API_KEYS=<comma-separated-list-of-api-keys-for-authentication-if-enabled>